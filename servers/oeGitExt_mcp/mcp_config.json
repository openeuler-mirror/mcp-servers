{
  "mcpServers": {
    "oeGitExt_mcp": {
      "command": "uv",
      "args": [
        "--directory",
        "YOUR_PATH/mcp-servers/servers/oeGitExt_mcp/src",
        "run",
        "oegitext_mcp.py",
        "--token=YOUR_GITEE_TOKEN"
      ],
      "disabled": false,
      "alwaysAllow": [
        "get_my_openeuler_project"
      ]
    }
  }
}
